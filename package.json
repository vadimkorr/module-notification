{
  "name": "module-notification",
  "version": "1.1.4",
  "description": "Plugin for displaying notifications inside specified html element containers (modules)",
  "main": "entry.js",
  "scripts": {
    "test": "karma start spec/karma.conf.js --single-run",
    "build:dev": "set NODE_ENV=dev && webpack --config ./config/webpack.config.js",
    "start:dev": "set NODE_ENV=dev && webpack --watch --config ./config/webpack.config.js",
    "build:prod": "set NODE_ENV=prod && webpack --config ./config/webpack.config.js",
    "jsdoc": "jsdoc src/js/ -d ./docs/jsdoc/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadimkorr/module-notification.git"
  },
  "keywords": [
    "notifications",
    "notification",
    "modules"
  ],
  "author": "Vadim Korr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vadimkorr/module-notification/issues"
  },
  "homepage": "https://github.com/vadimkorr/module-notification#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "browserify": "^16.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.2.0",
    "jasmine-core": "^3.5.0",
    "jsdoc": "^3.6.3",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.2",
    "watchify": "^3.11.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "cuid": "^1.3.8"
  }
}
